{
    "name": "Budibase code interceptor",
    "version": "0.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "app/background.js"
    },
    "content_scripts": [{
        "matches": ["https://*.budibase.app/*"],
        "js": ["app/pageScript.js"]
    }],
    "host_permissions": [
        "https://*.budibase.app/*"
    ],
    "permissions": [
        "webRequest",
        "scripting",
        "activeTab"
    ]
}